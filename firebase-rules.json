      ".write": "auth != null || auth.uid === 'BiipQIXcGccp4YcsoeIhulY1tfa2'",
      ".indexOn": ["timestamp"]
    },
    "user_strains": {
      ".read": "auth != null || auth.uid === 'BiipQIXcGccp4YcsoeIhulY1tfa2'",
      ".write": "auth.uid === 'BiipQIXcGccp4YcsoeIhulY1tfa2'",
      "$userId": {
