{
  "rules": {
    "admins": {
      ".read": "auth != null",
      ".write": false
    },
    "chat_messages": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["timestamp"]
    },
    "user_strains": {
      "$userId": {
        ".read": "$userId === auth.uid || root.child('admins').child('marcone').val() == true",
        ".write": "$userId === auth.uid || root.child('admins').child('marcone').val() == true"
      }
    },
    "online_users": {
      ".read": "auth != null",
      "$userId": {
        ".write": "$userId === auth.uid"
      }
    },
    "genobank": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}