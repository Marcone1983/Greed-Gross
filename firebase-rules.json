{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "chat_messages": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$messageId": {
        ".validate": "newData.hasChildren(['username', 'message', 'timestamp', 'userId'])"
      }
    },
    "online_users": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$userId": {
        ".write": "auth != null"
      }
    },
    "user_strains": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$userId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    }
  }
}